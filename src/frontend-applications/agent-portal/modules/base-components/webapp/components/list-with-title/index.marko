<div.list-with-title>
    <b><translation-string pattern=input.title/></b>
    <ul>
        <for|text| of=input.list>
            <li>
                <if(input.doNotTranslateList)>${text}</if>
                <else>
                    <translation-string pattern=text/>
                </else>
            </li>
        </for>
    </ul>
</div>
