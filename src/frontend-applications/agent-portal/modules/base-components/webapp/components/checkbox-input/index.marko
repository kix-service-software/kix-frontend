<if(state.field)>
    <div class="form-check form-switch">
        <input
            type="checkbox"
            checked=state.checked
            on-click("checkboxClicked")
            disabled=(state.field.readonly)
            class="form-check-input checkbox-input"
        >
    </div>
</if>
