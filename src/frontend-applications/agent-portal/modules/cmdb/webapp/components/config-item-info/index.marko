<if(state.configItem)>
    <div.ci-label-container>
        <object-property-label object=state.configItem property="Name" labelProvider=state.labelProvider/>
        <object-property-label object=state.configItem property="Number" labelProvider=state.labelProvider/>
        <object-property-label object=state.configItem property="CurDeplStateID" labelProvider=state.labelProvider showIcon=true/>
