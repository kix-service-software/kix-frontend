<if(state.table)>
    <widget
        instanceId=state.instanceId
        title=state.title
        contentActions=state.actions
    >
        <@content>
            <div.assigned-contacts-container>
                <div.contact-filter>
                    <filter filterCount=state.filterCount on-filter("filter")/>
                </div>
                <kix-table table=state.table/>
            </div>
        </@content>
    </widget>
</if>
<else><table-placeholder/></else>
