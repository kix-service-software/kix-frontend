<span.ticket-contact-input>
    <if(state.field)>
        <form-list
            placeholder=state.placeholder
            searchCallback=state.searchCallback
            autoCompleteConfiguration=state.autoCompleteConfiguration
            on-focusLost("focusLost")
            on-nodesChanged("nodesChanged")
            invalid=state.invalid
            freeText=true
            readonly=state.field.readonly
            actions=state.actions
            loadNodes=state.loadNodes
            treeId=state.treeId/>
    </if>
</span>
