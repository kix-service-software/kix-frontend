<div class="article-body input-style">
    <if(state.prepared)>
        <editor
            resize
            value=state.currentValue
            on-focusLost("valueChanged")
            on-valueChanged("valueChanged")
            noImages=state.noImages
            key=state.editorId
            readOnly=state.readonly
            useReadonlyStyle=state.readonly
        />
    </if>
</div>
