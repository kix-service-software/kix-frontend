            <link rel='shortcut icon' type='image/png' sizes='32x32' href='/static/img/kix_default.png'/>
        </else>
        <link rel='stylesheet' href='/static/thirdparty/bootstrap-5.0.2-dist/css/bootstrap.min.css'/>
