<widget
    instanceId=state.instanceId
    title=(state.widgetConfiguration ? state.widgetConfiguration.title : "")
    contentActions=state.actions
>
    <@content>
        <if(state.systemAddress)>
            <div class="system-address-information">
                <div class="information-list">
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="Name"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="Realname"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="QueueID"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="Comment"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="ValidID"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="CreateTime"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="CreateBy"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="ChangeTime"
                            labelProvider=state.labelProvider
                        />
                    </div>
                    <div>
                        <object-property-label
                            object=state.systemAddress
                            property="ChangeBy"
                            labelProvider=state.labelProvider
                        />
                    </div>
                </div>
            </div>
        </if>
    </@content>
</widget>
