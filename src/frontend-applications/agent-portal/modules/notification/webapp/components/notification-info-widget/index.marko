<widget
    instanceId=state.instanceId
    title=(state.widgetConfiguration ? state.widgetConfiguration.title : "")
    contentActions=state.actions>
    <@content>
        <div.notification-information>
            <object-information object=state.notification properties=state.properties flat=false/>
            <div.event-list>
                <span>
                    <translation-string pattern="Translatable#Events"/>:
                </span>
                <label-list labels=state.eventLabels removeLabels=false/>
            </div>
        </div>
    </@content>
</widget>
