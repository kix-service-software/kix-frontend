<div class="notification-info-container">
   <if(state.notification)>
      <div class="notification-info-teaser">
         ${state.createTime} - ${state.notification.teaserText}
      </div>
      <iframe
         class="w-100 h-100 bg-white"
         on-load("viewLoaded")
         id=state.frameId>
         <body/>
      </iframe>
   </if>
</div>
